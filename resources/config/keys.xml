<keys>
	<key name="Left"   key="Left"/>
	<key name="Right"  key="Right"/>
	<key name="Up"     key="Up"/>
	<key name="Down"   key="Down"/>

	<key name="A"      key="X"/>
	<key name="B"      key="Z"/>

	<key name="Start"  key="Return"/>
	<key name="Select" key="Backspace"/>
</keys>
